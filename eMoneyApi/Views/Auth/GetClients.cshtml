@model eMoneyApi.Models.ClientInformation
@{
    ViewBag.Title = "ClientList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div style="padding: 15px 15px 0px 15px; height:85%;">
    <h2>Clients</h2>
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>Client GUID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Cell Phone</th>
                <th>Owner ID</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var client in Model.clients.clientList.Where(cl => cl.ownerId == "ed9ed811-8e44-4c02-8485-a2abd47565a7"))
            {
            <tr>
                <td>
                    @Html.ActionLink(client.id, "GetSpecificClient", new { id = client.id })
                </td>
                <td>@client.firstName</td>
                <td>@client.lastName</td>
                <td>@client.email</td>
                <td>@client.cellPhone</td>
                <td>@client.ownerId</td>
            </tr>
            }
        </tbody>
    </table>
    <div class="text-center">
        <button type="button" class="btn btn-primary" onclick="@("window.location.href='" + @Url.Action("Navigate", "Auth") + "'");">Make Different Calls</button>
    </div>
</div>
